<div class="container col-lg-6 col-md-6 col-sm-6 col-xs-6">
    <div class="jumbotron">
        <h1>Login</h1>
        @model LoginRegistration.Models.LoginUser
        <form asp-controller="User" asp-action="login" method="post">
            <label asp-for="LogEmail"></label>
            <input class="form-control" asp-for="LogEmail" />
            <span asp-validation-for="LogEmail"></span>
            <br>
            <label asp-for="LogPassword"></label>
            <input class="form-control" asp-for="LogPassword" />
            <span asp-validation-for="LogPassword"></span>
            <br>
            <button class="btn btn-primary" type="submit">Log In</button>
        </form>
    </div>
</div>